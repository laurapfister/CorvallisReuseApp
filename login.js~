

$(#login).click(function(){
    $.ajax({type:"POST", 
	    url: "login.php", 
	    data: {user: $(#user).val(), password: $(#password).val()}, 
	    success:function(data){
		window.location.href = "CorvallisReuseReapir.html";
	    },
	    error:function(data){
		alert("Invalid Login Information");
	    }
	   });
    });
	    
